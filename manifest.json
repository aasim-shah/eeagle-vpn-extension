{
  "manifest_version": 3,
  "name": "Eagle-VPN",
  "version": "1.0",
  "description": "A lightweight VPN proxy extension",
  "permissions": ["proxy", "storage"],
  "host_permissions": ["https://*/*", "http://*/*"],
  "icons": {
    "16": "logo.png",
    "32": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo.png",
      "32": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    }
  }
}
